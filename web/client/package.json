  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitest/coverage-c8": "^0.29.2",
    "@vitest/ui": "^0.29.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^21.1.0",
    "resize-observer-polyfill": "^1.5.1",
